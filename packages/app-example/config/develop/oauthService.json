{
  "token": {
    "algorithm": "RS256",
    "privateKey": "certs/oauth_key.priv",
    "publicKey": "certs/oauth_key.pub",
    "issuer": "oauth:server",
    "audience": "user:access:token",
    "expireIn": "15m"
  },
  "refreshToken": {
    "algorithm": "RS256",
    "privateKey": "certs/oauth_key.priv",
    "publicKey": "certs/oauth_key.pub",
    "issuer": "oauth:server",
    "audience": "user:access:refresh:token",
    "expireIn": "14d"
  }
}
